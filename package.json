{
  "name": "edutvum-exam",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --code-coverage --single-run",
    "dev": "ng serve -e dev",
    "stage": "ng serve -e stage",
    "prod": "ng serve -e prod",
    "build": "ng build --prod -e prod",
    "lint": "ng lint",
    "deploy": "ng build --prod -e prod && firebase deploy --only hosting",
    "export": "firebase database:get / edutvum-exam > cmd-line/edutvum-exam-export-$(date +%Y%m%d_%H).json",
    "verify": "tsc cmd-line/verify.ts && node cmd-line/verify.js cmd-line/edutvum-exam-import.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0",
    "@angular/cdk": "^2.0.0-beta.11",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/material": "^2.0.0-beta.11",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/platform-server": "5.0.0",
    "@angular/router": "5.0.0",
    "angularfire2": "^4.0.0-rc0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "firebase": "^4.3.0",
    "jquery": "^3.2.1",
    "jsonfile": "^3.0.1",
    "jsonschema": "^1.2.0",
    "moment": "^2.18.1",
    "ng2-pagination": "^2.0.1",
