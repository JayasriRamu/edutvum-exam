    "@angular/compiler-cli": "^5.2.9",
    "@types/jasmine": "2.5.51",
    "@types/node": "^7.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "2.6.3",
    "jasmine-spec-reporter": "4.1.0",
    "jsonpp": "^1.0.2",
    "karma": "1.7.0",
    "karma-chrome-launcher": "^2.0.0",
